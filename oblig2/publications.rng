<element xmlns="http://relaxng.org/ns/structure/1.0" name="publications">
	<oneOrMore>
		<element name="article">
			<element name="title">
				<text/>
			</element>
			<element name="author">
				<text/>
			</element>
			<element name ="journal">
				<text/>
				<element name="volume">
					<data type="integer" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
				</element>
				<element name="number">
					<data type="integer" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
				</element>
			</element>
			<element name="pages">
				<text/>
			</element>
			<element name="cited">
				<data type="integer" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
			</element>
			<element name="year">
				<data type="integer" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"/>
			</element>
		</element>
		<element name="book">
			<attribute name="author"></attribute>
		</element>
		<element name="chapter">
			<attribute name="author"></attribute>
		</element>
	</oneOrMore>
</element>